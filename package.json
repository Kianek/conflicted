{
  "name": "conflicted",
  "version": "1.0.0",
  "description": "An app for maintaining a running list of previous clients of a law firm, for the purpose of checking whether future representation of a potential client would present a conflict of interest",
  "main": "index.js",
  "author": "Keanu Allridge",
  "license": "MIT",
  "scripts": {
    "build": "./node_modules/.bin/babel src --out-dir lib",
    "start": "node lib",
    "dev": "concurrently \"./node_modules/.bin/babel src -w --out-dir lib\" \"nodemon lib\"",
    "test": "mocha"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "core-js": "^3.11.1",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "faker": "^5.5.3",
    "helmet": "^4.5.0",
    "mongoose": "^5.12.5",
    "morgan": "^1.10.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "regenerator-runtime": "^0.13.7",
    "validator": "^13.6.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.16",
    "@babel/core": "^7.13.16",
    "@babel/preset-env": "^7.13.15",
    "@babel/register": "^7.13.16",
    "chai": "^4.3.4",
    "concurrently": "^6.0.2",
    "mocha": "^8.3.2",
    "mongodb-memory-server-core": "^6.9.6",
    "nodemon": "^2.0.7"
  }
}
